    <property>
      <code>require(['jquery'], ($) =&gt; {
  $(document).ready(() =&gt; {
    // This element is added by the configurable class, but I couldn't find a translation key in the documentation.
    $("#HAnalytics\\.Code\\.AggregatorConfigs\\.LastSeenConfig\\.Configuration\\:").hide()

    // Function to toggle the visibility of startDate and endDate along with their labels
    const toggleDateInputs = (value) =&gt; {
      const isCustom = value === "custom";

      // Select the startDate and endDate inputs and their labels
      const startDateInput = $('#Analytics\\.Code\\.AggregatorConfigs\\.LastSeenConfig\\.Configuration_Analytics\\.Code\\.AggregatorConfigs\\.LastSeenConfig\\.ConfigurationClass_0_startDate');
      const startDateLabel = $('label[for="Analytics.Code.AggregatorConfigs.LastSeenConfig.Configuration_Analytics.Code.AggregatorConfigs.LastSeenConfig.ConfigurationClass_0_startDate"]').closest('dt');

      const endDateInput = $('#Analytics\\.Code\\.AggregatorConfigs\\.LastSeenConfig\\.Configuration_Analytics\\.Code\\.AggregatorConfigs\\.LastSeenConfig\\.ConfigurationClass_0_endDate');
      const endDateLabel = $('label[for="Analytics.Code.AggregatorConfigs.LastSeenConfig.Configuration_Analytics.Code.AggregatorConfigs.LastSeenConfig.ConfigurationClass_0_endDate"]').closest('dt');

      // Show or hide the inputs and their labels
      if (isCustom) {
        startDateInput.show();
        startDateLabel.show();
        endDateInput.show();
        endDateLabel.show();
      } else {
        startDateInput.hide();
        startDateLabel.hide();
        endDateInput.hide();
        endDateLabel.hide();
      }
    };

    const initialValue = $('#Analytics\\.Code\\.AggregatorConfigs\\.LastSeenConfig\\.Configuration_Analytics\\.Code\\.AggregatorConfigs\\.LastSeenConfig\\.ConfigurationClass_0_timeIntervalForStatistics').val();
    toggleDateInputs(initialValue);

    // Event listener for changes to the dropdown
    $('#Analytics\\.Code\\.AggregatorConfigs\\.LastSeenConfig\\.Configuration_Analytics\\.Code\\.AggregatorConfigs\\.LastSeenConfig\\.ConfigurationClass_0_timeIntervalForStatistics').on('change', function () {
      toggleDateInputs($(this).val());
    });
  });
});
</code>
